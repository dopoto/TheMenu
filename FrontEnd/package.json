{
    "name": "front-end",
    "version": "0.0.1",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --port 4210 --ssl --ssl-cert %APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem --ssl-key %APPDATA%\\ASP.NET\\https\\%npm_package_name%.key",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "cypress-dev1": "concurrently \"npm run start\" \"cypress open --config-file cypress-dev1-config\"",
        "cypress-ci": "concurrently \"npm run start\" \"cypress open --config-file cypress-ci\"",
